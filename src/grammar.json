{
  "name": "asm6809",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_line"
      }
    },
    "_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_expression"
        },
        {
          "type": "SYMBOL",
          "name": "_line_break"
        }
      ]
    },
    "_line_break": {
      "type": "STRING",
      "value": "\n"
    },
    "_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_expression"
            },
            {
              "type": "SYMBOL",
              "name": "_operator"
            },
            {
              "type": "SYMBOL",
              "name": "_term"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_term"
        }
      ]
    },
    "_term": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "("
            },
            {
              "type": "SYMBOL",
              "name": "_expression"
            },
            {
              "type": "STRING",
              "value": ")"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "symbol"
        },
        {
          "type": "SYMBOL",
          "name": "constant"
        }
      ]
    },
    "constant": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_decimal"
        },
        {
          "type": "SYMBOL",
          "name": "_octal"
        },
        {
          "type": "SYMBOL",
          "name": "_hexadecimal"
        },
        {
          "type": "SYMBOL",
          "name": "_binary"
        }
      ]
    },
    "_decimal": {
      "type": "PATTERN",
      "value": "[0-9]+"
    },
    "_octal": {
      "type": "PATTERN",
      "value": "\\@[0-7]+"
    },
    "_hexadecimal": {
      "type": "PATTERN",
      "value": "\\$[a-fA-Z0-9]+"
    },
    "_binary": {
      "type": "PATTERN",
      "value": "\\%[01]+"
    },
    "symbol": {
      "type": "PATTERN",
      "value": "[a-zA-Z\\._][a-zA-Z0-9\\._\\$]*"
    },
    "_operator": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "operator"
        }
      ]
    },
    "operator": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "+"
        },
        {
          "type": "STRING",
          "value": "-"
        },
        {
          "type": "STRING",
          "value": "*"
        },
        {
          "type": "STRING",
          "value": "/"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "externals": [],
  "inline": []
}